{
    "name": "swisnl/laravel-api",
    "type": "library",
    "description": "API in Laravel",
    "keywords": [
        "swisnl",
        "laravel-api"
    ],
    "homepage": "http://www.swis.nl",
    "license": "MIT",
    "authors": [
        {
            "name": "Dylan de Wit",
            "email": "ddewit@swis.nl",
            "homepage": "https://www.swis.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "dimsav/laravel-translatable": "^7.3",
        "laravel/framework": "~5.4",
        "laravel/tinker": "^1.0",
        "neomerx/json-api": "^1.0",
        "spatie/laravel-permission": "^2.5",
        "InfyOmLabs/laravel-generator": "dev-master",
        "infyomlabs/adminlte-templates": "dev-master",
        "infyomlabs/swagger-generator": "dev-master",
        "laravelcollective/html": "^5.4",
        "jlapp/swaggervel": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.4",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Swis\\LaravelApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
